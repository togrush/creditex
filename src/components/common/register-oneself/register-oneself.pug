mixin register-oneself(title1, title2, buttonText, form)
  section.section.register-oneself
    .container
      .section__wrapper.register-oneself__wrapper
        .title-container
          h4 #{title1}
          h4 #{title2}
        include UIKit/button/button.pug
        if !form
          +button('register-oneself-button', './registration.html', 'link', 'secondary', buttonText)(class='register-oneself__button')
        else
          +button('register-oneself-button', '', 'link', 'secondary', buttonText)(class='register-oneself__button' data-modal-target="#modal" tabindex="0" role="button")
